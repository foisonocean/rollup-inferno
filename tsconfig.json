{
  "compilerOptions": {
    /* Basic Options */
    "target": "es5",
    "module": "es2015",
    "jsx": "preserve",
    "importHelpers": true,
    "downlevelIteration": true,

    /* Strict Type-Checking Options */
    "strict": true,

    /* Module Resolution Options */
    "moduleResolution": "node",
    "baseUrl": "./src",
    "esModuleInterop": false
  }
}
